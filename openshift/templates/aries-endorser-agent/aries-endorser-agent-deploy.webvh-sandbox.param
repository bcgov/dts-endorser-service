# WALLET_SEED=[a-zA-Z0-9]{32}
# WALLET_DID=[a-zA-Z0-9]{16}
WALLET_SUFFIX=-webvh-sandbox
ENDORSER_SERVICE_HOST=aries-endorser-api
ENDORSER_SERVICE_PORT=5000
CONTROLLER_WEBHOOK_URL=http://aries-endorser-api-webvh-sandbox:5000/webhook#tbd
LOG_LEVEL=INFO
MIN_REPLICAS=1
MAX_REPLICAS=2
CPU_REQUEST=10m
CPU_LIMIT=100m
MEMORY_REQUEST=160Mi
MEMORY_LIMIT=256Mi
